{
    "name": "xad",
    "manifest_version": 2,
    "version": "1.0",
    "description": "chrome删除广告的扩展",
    "browser_action": {
        "default_icon": "icon.png"
    },
    "content_scripts": [
        {
            "matches": ["http://blog.csdn.net/*", "http://blog.csdn.net/*", "http://www.iplaysoft.com/*"],
            "js": ["jquery.js", "script.js"]
        }
    ]
}